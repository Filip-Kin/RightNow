{
  "expo": {
    "name": "RightNow",
    "slug": "RightNow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/icon-512.png",
    "scheme": "rightnow",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.filipkin.rightnow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/icon-512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.filipkin.rightnow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "assets/images/icon-512.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
